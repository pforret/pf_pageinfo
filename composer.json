{
    "name": "pforret/pf_pageinfo",
    "version": "1.1.1",
    "description": "retrieve content and metadata from any URL",
    "keywords": [
        "pforret",
        "pf_pageinfo"
    ],
    "homepage": "https://github.com/pforret/pf_pageinfo",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Forret",
            "email": "peter@forret.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Pforret\\PfPageinfo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pforret\\PfPageinfo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
